{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "Advent of Code",
  "scripts": {
    "docker:build": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose --env-file ./docker/.env -f docker/docker-compose.yaml up --build -d",
    "docker:boot": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose --env-file ./docker/.env -f docker/docker-compose.yaml up -d",
    "docker:stop": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose --env-file ./docker/.env -f docker/docker-compose.yaml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nesci28/AdventOfCode.git"
  },
