  "bugs": {
    "url": "https://github.com/Nesci28/AdventOfCode/issues"
  },
  "homepage": "https://github.com/Nesci28/AdventOfCode#readme",
  "dependencies": {
    "flat": "^5.0.2",
    "generatorics": "^1.1.0",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
