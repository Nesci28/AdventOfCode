{
    "files.exclude": {
      "**/node_modules": false
    },
    "peacock.remoteColor": "#146B3A",
    "javascript.preferences.quoteStyle": "double",
    "typescript.preferences.quoteStyle": "double",
    "typescriptHero.imports.stringQuoteStyle": "\"",
    "editor.formatOnSave": true,
    "eslint.alwaysShowStatus": true,
    "[jsonc]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[typescript]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[html]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[scss]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[javascript]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "typescriptHero.imports.organizeOnSave": true,
    "workbench.colorTheme": "Dracula Refined",
    "workbench.iconTheme": "material-icon-theme",
    "peacock.color": "#dd0531",
    "indentRainbow.includedLanguages": ["yml", "yaml", "YML", "YAML"],
    "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#1c9651",
      "activityBar.activeBorder": "#6623bc",
      "activityBar.background": "#1c9651",
      "activityBar.foreground": "#e7e7e7",
      "activityBar.inactiveForeground": "#e7e7e799",
      "activityBarBadge.background": "#6623bc",
      "activityBarBadge.foreground": "#e7e7e7",
      "statusBar.background": "#146b3a",
      "statusBar.foreground": "#e7e7e7",
      "statusBarItem.hoverBackground": "#1c9651",
      "titleBar.activeBackground": "#146b3a",
      "titleBar.activeForeground": "#e7e7e7",
      "titleBar.inactiveBackground": "#146b3a99",
      "titleBar.inactiveForeground": "#e7e7e799",
      "sash.hoverBorder": "#1c9651",
      "statusBarItem.remoteBackground": "#146b3a",
      "statusBarItem.remoteForeground": "#e7e7e7"
    },
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.tabSize": 2
  }