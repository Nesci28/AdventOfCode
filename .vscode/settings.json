{
  "files.exclude": {
    "**/generators/**": true,
    "**/minio/**": true,
    "**/mongo/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.angular/**": true,
    "**/home/**": true
  },
  "files.watcherExclude": {
    "**/generators/**": true,
    "**/minio/**": true,
    "**/mongo/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.angular/**": true,
    "**/home/**": true,
    "package-lock.json": true
  },
  "search.exclude": {
    "**/generators/**": true,
    "**/minio/**": true,
    "**/mongo/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.angular/**": true,
    "**/home/**": true,
    "package-lock.json": true
  },
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "typescriptHero.imports.stringQuoteStyle": "\"",
  "eslint.alwaysShowStatus": true,
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  // "typescriptHero.imports.organizeOnSave": true,
  "editor.detectIndentation": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dracula Refined",
  "editor.tabSize": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "todohighlight.keywords": ["LOGIC_CHECK:"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "angular.enable-strict-mode-prompt": true,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "files.autoSave": "onFocusChange",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#c3c5c7",
    "activityBar.activeBorder": "#c7588f",
    "activityBar.background": "#c3c5c7",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#c7588f",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#a9abae",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#8f9195",
    "titleBar.activeBackground": "#a9abae",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a9abae99",
    "titleBar.inactiveForeground": "#15202b99",
    "sash.hoverBorder": "#c3c5c7",
    "statusBarItem.remoteBackground": "#a9abae",
    "statusBarItem.remoteForeground": "#15202b",
    "commandCenter.border": "#15202b99"
  },
  "peacock.remoteColor": "#A9ABAE",
  "peacock.color": "#f9e64f"
}
